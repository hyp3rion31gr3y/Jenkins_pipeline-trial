pipeline{
	agent any
		stages{
			stage('Build Stage'){
				steps{
					echo 'It is in Building stage !!!!'
				}

			}
			stage('Test Stage'){
				steps{
					echo 'It is in Testing Stage !!!'
				}
			}

			stage('Display Stage'){
				steps{
					echo 'It is in Displaying Stage !!!!'
					echo 'Successfully made a demo declarative pipeline!!!'
				
				}
			}
		}
}
